@model IEnumerable<SfDesk.Models.Ledger>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_dashLayout.cshtml";
}


<div class="row">
    <div class="col-lg-12">
        <div class="card alert">
            <button type="button" class="btn btn-primary  btn-addon m-b-10 m-l-5" onclick="location.href='@Url.Action("add", "user")'"><i class="ti-plus"></i>Create   new </button>
            <div class="bootstrap-data-table-panel">
                <div class="table-responsive">
                    <table id="bootstrap-data-table-export" class="table table-striped table-bordered">
                        <thead>
                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.PI_ID)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Date)
                                </th>

                                <th>
                                    @Html.DisplayNameFor(model => model.Party_Name)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Account_Name)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Flag)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Amount)
                                </th>                               
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.PI_ID)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Date)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Party_Name)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Account_Name)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Flag)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Amount)
                                    </td>
                                </tr>

                            }
                        </tbody>
                    </table>
                </div>
            </div>

          

        </div>
        <!-- /# card -->
    </div>
    <!-- /# column -->
</div>