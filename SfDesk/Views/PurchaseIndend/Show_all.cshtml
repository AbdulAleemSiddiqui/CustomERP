@model IEnumerable<SfDesk.Models.P_Indent>

@{
    ViewBag.Title = "Show_all";
    Layout = "~/Views/Shared/_dashLayout.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <div class="card alert">
            <button type="button" class="btn btn-primary btn-flat btn-addon m-b-10 m-l-5" onclick="location.href='@Url.Action("master", "Purchaseintend")'"><i class="ti-plus"></i>Create   new </button>
            <div class="bootstrap-data-table-panel">
                <div class="table-responsive">
                    <table id="bootstrap-data-table-export" class="table table-striped table-bordered">
                        <thead>
                            <tr>
                               
                                <th>
                                    @Html.DisplayNameFor(model => model.PI_NO)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.PI_Date)
                                </th>
                              
                                <th>
                                    @Html.DisplayNameFor(model => model.PI_Item_Cat_Name)
                                </th>
                               
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                  
                                    <td>
                                        @Html.DisplayFor(modelItem => item.PI_NO)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.PI_Date)
                                    </td>
                                   
                                    <td>
                                        @Html.DisplayFor(modelItem => item.PI_Item_Cat_Name)
                                    </td>
                                  
                                    <td>
                                        @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
                                        @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                                        @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        <!-- /# card -->
    </div>
    <!-- /# column -->
</div>
